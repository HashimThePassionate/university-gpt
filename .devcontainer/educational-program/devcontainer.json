{
    "name": "Education Program Container",
    "dockerComposeFile": ["../../docker-compose.yml"],
    "service": "educational-program", // This Shall be same as the service name in docker-compose.yml
    "shutdownAction": "none",
    "workspaceFolder": "/educational-program",  // Changed from "/workspace" to match the volume mount in docker-compose.yml
	"postCreateCommand": "poetry install",
    "forwardPorts": [
        "8000:8000"
    ]
}